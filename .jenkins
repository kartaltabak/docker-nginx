//noinspection GroovyUnusedAssignment
@Library('github-jenkins-pipeline') _

dockerBuildPipeline(
        mainBranchCron: '@monthly',
        imageBuilder: [
                baseImage        : 'nginx:latest',
                registryRepoName : 'kartaltabak/nginx',
                imageTestCommands: [
                        'nginx -h',
                        'ping -h'
                ]
        ]
)
